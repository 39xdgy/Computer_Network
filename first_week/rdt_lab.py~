class packet:
    def __init__(self, seq_num, ack_num, checknum, data):
        self.seq_num = seq_num
        self.ack_num = ack_num
        self.checknum = checknum
        self.data = data

    def get_seq(self):
        return self.seq_num
    def get_act(self):
        return self.act_num
    def get_checknum(self):
        return self.checknum
    def get_data(self):
        return self.data


if __name__ = "__main__":
    s = packet(4, 5, 6, 7)
